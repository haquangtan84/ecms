<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UIFCCForm>
    <msg>
      <name-not-allowed>Šis pavadinimas yra neleidžiamas.</name-not-allowed>
      <in-versioning>Prašome patikrinti dabartinį mazgą prieš pridedant naują.</in-versioning>
      <cannot-save>Įvyko klaida išsaugojant mazgą.</cannot-save>
      <saved-successfully>{0}</saved-successfully>
      <not-support>Nepavyko įkelti '{0}' šablono.</not-support>
      <not-add-folder>Tik failai gali būti pridedami į šį aplanką.</not-add-folder>
      <access-denied>Prieiga uždrausta. Jūs neturite pridėjimo teisės prie '{0}'.</access-denied>
      <path-not-found>Kelias '{0}' ištrintas arba klaidingas.</path-not-found>
      <node-locked>Mazgas '{0}' yra užrakintas. Prašome atrakinti šį mazgą prieš dokumentų kūrimą.</node-locked>
      <node-isExist>Šis mazgas jau egzistuoja '{0}' ir negali būti tuo pačiu pavadinimu.</node-isExist>
      <get-dialog-path>Įvyko klaida gaunant dialogo kelią.</get-dialog-path>
      <new-jcr-template>Kuriant naują šabloną įvyko klaida.</new-jcr-template>
    </msg>
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
  </UIFCCForm>

  <UIFCCConfig>
    <label>
      <UIFCCSaveLocationField>Išsaugoti vietą</UIFCCSaveLocationField>
      <UIFCCWorkspaceFormSelectBox>Pasirinkti darbo sritį</UIFCCWorkspaceFormSelectBox>
      <UIFCCTemplateField>Šablono parinktys</UIFCCTemplateField>
      <UIFCCLocationFormInputAction>Vieta išsaugojimui</UIFCCLocationFormInputAction>
      <UIFCCTemplateFormSelectBox>Pasirinkti šabloną</UIFCCTemplateFormSelectBox>
      <UIFCCSaveFormStringInput>Pasirinktinis išsaugojimo mygtukas</UIFCCSaveFormStringInput>
      <UIFCCMessageFormTextareaInput>Pasirinktinė išsaugojimo žinutė</UIFCCMessageFormTextareaInput>
      <UIFCCRedirectFormCheckboxInput>Nukreipti:</UIFCCRedirectFormCheckboxInput>
      <UIFCCRedirectPathFormStringInput>Nukreipimo kelias:</UIFCCRedirectPathFormStringInput>
      <UIFCCActionField>Veiksmai</UIFCCActionField>
    </label>
    <msg>
      <save-successfully>Saved to portlet preferences successfully.</save-successfully>
      <item-not-found>Šis kelias yra klaidingas.</item-not-found>
      <fileType-empty>Please select a file type before saving this configuration.</fileType-empty>
      <ws-empty>Please select one workspace.</ws-empty>
      <action-empty>Please select a folder with an action.</action-empty>
      <message-empty>The "Custom Save Message" field is required in case you do not choose the redirect option, or the path is empty.</message-empty>
      <get-template>An error occurred while accessing the template.</get-template>
      <set-template-option>An error occurred while setting the template option.</set-template-option>
      <do-select>An error occurred while selecting the location.</do-select>
      </msg>
    <action>
      <Save>#{word.save}</Save>
    </action>
  </UIFCCConfig>

  <UIFCCLocationFormInputAction>
    <title>
      <SelectPath>Select Path</SelectPath>
    </title>
  </UIFCCLocationFormInputAction>

  <UIFCCActionList>
    <header>
      <name>Name</name>
      <description>Aprašymas</description>
      <instanceOf>Instance of</instanceOf>
      <action>Veiksmas</action>
    </header>
    <action>
      <Add>Pridėti</Add>
    </action>
    <msg>
      <empty>No actions</empty>
      <remove-popup-first>You must close the edit pop-up before deleting this node.</remove-popup-first>
      <confirm-delete-action>Are you sure you want to delete this action?</confirm-delete-action>
      <template-empty>The template of '{0}' is null or empty.</template-empty>
      <template-null>The template is null or empty, or you do not have permission to access this template.</template-null>
      <access-denied>Access denied. You do not have permission to perform this action.</access-denied>
    </msg>
  </UIFCCActionList>

  <UIFCCSelectorPopupWindow>
    <title>
      <UIOneNodePathSelector>Select Location</UIOneNodePathSelector>
    </title>
  </UIFCCSelectorPopupWindow>

  <UIWorkspaceList>
    <label>
      <workspaceName>Workspace:</workspaceName>
      <rootNodePath>Root Path:</rootNodePath>
      <rootNodeInfo>Root node</rootNodeInfo>
    </label>
  </UIWorkspaceList>

  <rootNodeInfo>
    <title>
      <AddRootNode>Add Root Node</AddRootNode>
    </title>
  </rootNodeInfo>

  <UITreeList>
    <info>
      <child-not-found>This node does not have any child ones.</child-not-found>
    </info>
    <header>
      <name>Name</name>
      <action>Action</action>
    </header>
  </UITreeList>

  <UIFCCActionPopupWindow>
    <title>
      <UIFCCActionContainer>Pridėti veiksmą</UIFCCActionContainer>
    </title>
  </UIFCCActionPopupWindow>

  <UIFCCActionForm>
    <title>Add/Edit Action</title>
    <action>
      <Save>#{word.save}</Save>
      <Close>#{word.close}</Close>
    </action>
    <msg>
      <repository-exception>Cannot save this action. The name provided must not have an index as its final element.</repository-exception>
      <numberformat-exception>Cannot save this action. The name provided must not have '[' or ']'.</numberformat-exception>
      <existed-action>The action name '{0}' already exists. Please select another name.</existed-action>
      <unable-add-action>Unable to add this action.</unable-add-action>
      <unable-add>Unable to add this action. This node is root or you do not have permision.</unable-add>
      <not-support>The dialog '{0}' is not supported.</not-support>
      <no-permission-add>Access denied. You do not have the right to add an action here.</no-permission-add>
      <access-denied>Access denied. You do not have permission to access this template.</access-denied>
    </msg>
    <validator>
      <validator>Validator</validator>
      <destPath>With Path</destPath>
    </validator>
  </UIFCCActionForm>

  <UIFCCActionTypeForm>
    <label>
      <actionType>Create an Action of Type</actionType>
    </label>
    <msg>
      <not-support>Cannot load the template of '{0}'.</not-support>
      <do-not-permission>You do not have permission to load the template of '{0}'.</do-not-permission>
    </msg>
  </UIFCCActionTypeForm>

  <PopupComponent>
    <title>
      <UIOneNodePathSelector>Select Location</UIOneNodePathSelector>
      <UIPermissionSelector>Permission Browser</UIPermissionSelector>
      <UICategoriesSelector>Add Categories</UICategoriesSelector>
    </title>
  </PopupComponent>

  <DateTimeValidator>
    <msg>
      <Invalid-input>The field '{0}' with the value '{1}' is invalid.</Invalid-input>
    </msg>
  </DateTimeValidator>

  <UITabPane>
    <title>
      <UICategoriesSelectPanel>Pasirinkti kategorijas</UICategoriesSelectPanel>
      <UISelectedCategoriesGrid>Nurodytos kategorijos</UISelectedCategoriesGrid>
    </title>
  </UITabPane>

  <PopupComponent>
    <title>
      <UICategoriesSelector>Add Categories</UICategoriesSelector>
    </title>
  </PopupComponent>

  <UICategoriesSelector>
    <action>
      <close>#{word.close}</close>
    </action>
  </UICategoriesSelector>

  <PopupComponent>
    <title>
      <UIOneTaxonomySelector>Add Categories</UIOneTaxonomySelector>
    </title>
  </PopupComponent>

  <UITreeTaxonomyList>
    <label>
      <taxonomyTree>Category Tree</taxonomyTree>
    </label>
  </UITreeTaxonomyList>

  <UIFCCTaxonomyPopupWindow>
    <title>
      <UIOneTaxonomySelector>Category Selector</UIOneTaxonomySelector>
    </title>
  </UIFCCTaxonomyPopupWindow>
</bundle>

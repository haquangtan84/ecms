<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UIPresentationContainer>
    <title>
      <QuickEdit>Edit in Content Explorer</QuickEdit>
      <print>Напечатать эту страницу</print>
      <FastPublish>Опубликовать</FastPublish>
    </title>
    <label>
      <draft>Черновик</draft>
      <print>Печать</print>
    </label>
    <msg>
      <no-content>Содержимое не найдено.</no-content>
      <internal-server-error>При запросе данных с сервера произошла ошибка.</internal-server-error>
      <empty-title-error>Пожалуйста, укажите новое название.</empty-title-error>
    </msg>
  </UIPresentationContainer>
  <CorrectContentSelectorPopupWindow>
    <title>
      <UIContentSelectorOne>Выбрать содержимое</UIContentSelectorOne>
    </title>
  </CorrectContentSelectorPopupWindow>
  <UISingleContentViewerPortlet>
    <msg>
      <AccessControlException>У вас нет разрешения на доступ к режиму Редактирование.</AccessControlException>
    </msg>
  </UISingleContentViewerPortlet>
  <UISCVContextualRadioBox>
    <label>
      <Enable>Включено</Enable>
      <Disable>Отключено</Disable>
    </label>
  </UISCVContextualRadioBox>
  <UIPreferencesPopupWindows>
    <title>
      <UISCVPreferences>Content Detail Preferences</UISCVPreferences>
    </title>
  </UIPreferencesPopupWindows>
  <UISCVPreferences>
    <action>
      <Save>#{word.save}</Save>
      <Abort>Abort</Abort>
      <Cancel>#{word.cancel}</Cancel>
      <Print>Печать</Print>
      <Close>#{word.close}</Close>
    </action>
    <title>Content Detail Preferences</title>
    <label>
      <Back>Вернуться на предыдущую страницу</Back>
      <Edit>Редактировать</Edit>
      <Preferences>Preferences</Preferences>
      <Title>Content Detail Preferences</Title>
      <UISCVShowTitleConfigurationCheckBox>Показать заголовок</UISCVShowTitleConfigurationCheckBox>
      <UISCVShowDateConfigurationCheckBox>Показать дату</UISCVShowDateConfigurationCheckBox>
      <UISCVShowOptionBarConfigurationCheckBox>Show Option Bar</UISCVShowOptionBarConfigurationCheckBox>
      <DisplaySettingBlock>Настройки экрана</DisplaySettingBlock>
      <ContentPathBlock>Content Selection</ContentPathBlock>
      <UISCVConfigItemPathFormInputSet>Путь к содержимому</UISCVConfigItemPathFormInputSet>
    <UISCVContentPathConfigurationInputBox>Путь к содержимому</UISCVContentPathConfigurationInputBox>
      <Advance>Advanced</Advance>
      <ContextCaption>Dynamic Navigation</ContextCaption>
      <UISCVContextualRadioBox>Contextual Content</UISCVContextualRadioBox>
      <UISCVParameterInputBox>by</UISCVParameterInputBox>
      <PrintSettingBlock>Настройки печати</PrintSettingBlock>
      <UISCVPrintPageParameter>with</UISCVPrintPageParameter>
      <UISCVConfigPrintPageFormInputSet>Показать на странице</UISCVConfigPrintPageFormInputSet>
      <LockStatus>Lock</LockStatus>
      <CacheManagement>Content Visibility</CacheManagement>
      <UISCVCacheRadioBox>Share Content</UISCVCacheRadioBox>
    </label>
    <help>
      <ContentPathBlock>Select the content you want to show. This default content will be replaced dynamically if you activate the Dynamic Navigation option (see the Advanced mode below).</ContentPathBlock>
      <DisplaySettingBlock>You can show or hide the Option bar used to show the print link. Title and Date displays are also optional.</DisplaySettingBlock>
      <ContextualHelp>Activates Dynamic Navigation. The content can be changed dynamically by another URL-based parameter.</ContextualHelp>
      <ParameterHelp>This parameter will be used to get the content if the Dynamic Navigation is activated.</ParameterHelp>
    </help>
    <msg>
      <not-valid-path>The item path is invalid.</not-valid-path>
    </msg>
    <tab>
      <label>
        <UISCVConfigContentFormInputSet>Content Selection</UISCVConfigContentFormInputSet>
        <UISCVConfigDisplayFormInputSet>Настройки экрана</UISCVConfigDisplayFormInputSet>
        <UISCVConfigPrintFormInputSet>Настройки печати</UISCVConfigPrintFormInputSet>
        <UISCVConfigAdvancedFormInputSet>Advanced</UISCVConfigAdvancedFormInputSet>
      </label>
    </tab>
  </UISCVPreferences>
  <UISCVConfigItemPathFormInputSet>
    <title>
      <SelectFolderPath>Выберите путь к папке</SelectFolderPath>
      <AddPath>Add Path</AddPath>
    </title>
  </UISCVConfigItemPathFormInputSet>
  <UISCVConfigPrintPageFormInputSet>
    <title>
      <SelectTargetPage>Показать на странице</SelectTargetPage>
    </title>
  </UISCVConfigPrintPageFormInputSet>
  <PublicationStates>
    <archived>В архиве</archived>
    <obsolete>Obsolete</obsolete>
    <unpublished>Unpublished</unpublished>
    <enrolled>Enrolled</enrolled>
    <draft>Черновик</draft>
    <pending>Pending</pending>
    <approved>Approved</approved>
    <inreview>Under Review</inreview>
    <staged>Staged</staged>
    <published>Published</published>
  </PublicationStates>
  <UISCVCacheRadioBox>
    <label>
      <Enable>Restricted by Authentication</Enable>
      <Disable>Restricted by User roles</Disable>
    </label>
  </UISCVCacheRadioBox>
</bundle>

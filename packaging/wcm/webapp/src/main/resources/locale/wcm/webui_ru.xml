  </UIContentBrowsePathSelectorFolder>

  <UIContentSearchForm>
    <title>
      <Location>Site Name</Location>
      <SearchByName>Search by Name</SearchByName>
      <Fulltext>A word or phrase in context</Fulltext>
      <Property>Свойство</Property>
      <Contain>Contain</Contain>
      <FromDate>From Date</FromDate>
      <ToDate>To Date</ToDate>
      <DocumentType>Тип документа</DocumentType>
    </title>
    <tooltip>
      <AddMetadataType>Add Property</AddMetadataType>
      <AddNodeType>Add Node Type</AddNodeType>
    </tooltip>    
    <label>
      <option>
        <CREATED>Создано</CREATED>
        <MODIFIED>Дата последней модификации</MODIFIED>
      </option>
    </label>
    <action>
      <SearchWebContent>Search</SearchWebContent>
    </action>
    <msg>
      <time-to-late>The searching time is too long.</time-to-late>
      <empty-field>Please input a valid keyword for search.</empty-field>
      <invalid-format>Invalid date/time format in field '{0}'.</invalid-format>
      <invalid-date>"To Date" must be later than "From Date"</invalid-date>
      <invalid-nodeType>The node type "{0}" is invalid. Please check again.</invalid-nodeType>
      <invalid-keyword>Неверное ключевое слово. Некоторые символы не поддерживаются.</invalid-keyword>
    </msg>
  </UIContentSearchForm>

  <WebContentMetadataPopup>
    <title>
      <UIContentPropertySelector>Property Selector</UIContentPropertySelector>
    </title>
  </WebContentMetadataPopup>

  <UIContentPropertySelector>
    <action>
      <Add>Добавить</Add>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <label>

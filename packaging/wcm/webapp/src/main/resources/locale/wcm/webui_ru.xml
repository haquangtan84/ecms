    <action>
      <SaveCategories>#{word.save}</SaveCategories>
      <Cancel>#{word.cancel}</Cancel>
    </action>
  </UICLVContentSelectedGrid>

  <UIContentBrowsePathSelector>
    <header>
      <name>#{word.name}</name>
      <publicationstate>Publication State</publicationstate>
      <action>Действие</action>
    </header>
    <msg>
      <child-not-found>No Content Found</child-not-found>
    </msg>
  </UIContentBrowsePathSelector>

  <UIContentBrowsePathSelectorFolder>
    <header>
      <name>#{word.name}</name>
      <publicationstate>Publication State</publicationstate>
      <action>Действие</action>
    </header>
    <msg>
      <child-not-found>No Folder Found</child-not-found>

<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #####################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UICategoriesSelectPanel #
  #####################################################################
  -->
  <UITreeList>
    <header>
      <name>姓名</name>
      <action>新增</action>
    </header>
    <info>
      <child-not-found>找不到的子项 ！</child-not-found>
    </info>
  </UITreeList>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UISelectedCategoriesGrid #
  ######################################################################
  -->
  <UISelectedCategoriesGrid>
    <header>
      <categoryName>类别名称</categoryName>
      <categoryPath>类别路径</categoryPath>
      <action>#{word.action}</action>
    </header>
    <action>
      <SaveCategories>#{word.save}</SaveCategories>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <cannot-save>类别未被保存。</cannot-save>
      <have-saved>类别已成功保存</have-saved>
      <non-categories>类别是错误的。请选择适当的类别。</non-categories>
      <confirm-delete>你想要删除此类别吗？</confirm-delete>
    </msg>
  </UISelectedCategoriesGrid>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.selector.UIPermissionSelector           #
  ######################################################################
  -->
  <SelectUserOrGroup>
    <title>
      <UIPermissionUser>选择用户</UIPermissionUser>
      <UIPermissionMembership>选择成员</UIPermissionMembership>
    </title>
  </SelectUserOrGroup>
  <UIPermissionUser>
    <label>
      <selectUser>选择用户</selectUser>
      <selectGroup>浏览并选择一个组</selectGroup>
    </label>
  </UIPermissionUser>
  <UIPermissionMembership>
    <label>
      <selectUser>选择成员</selectUser>
      <selectGroup>浏览并选择一个组</selectGroup>
    </label>
  </UIPermissionMembership>
</bundle>
